{
  "name": "Hoopa-ShibCard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "pnpm run partytown && astro dev",
    "build": "pnpm run partytown && astro build",
    "preview": "pnpm run partytown && astro preview --port 9000",
    "astro": "astro",
    "partytown": "partytown copylib public/~partytown",
    "pre-commit-lint": "pnpm lint-staged",
    "pre-push-lint": "pnpm build",
    "prepare": "cd .. && husky install app/.husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "^18.6.4",
    "@types/react": "18.0.13",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "~5.24.0",
    "@typescript-eslint/parser": "~5.24.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "8.1.0",
    "eslint": "~8.15.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-astro": "^0.17.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-mdx": "^2.0.2",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.0",
    "jest": "27.5.1",
    "lint-staged": "13.0.2",
    "postcss": "^8.4.16",
    "prettier": "2.6.2",
    "prettier-plugin-astro": "^0.5.0",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^27.0.0",
    "ts-node": "10.8.0",
    "typescript": "4.7.2"
  },
  "dependencies": {
    "@astrojs/react": "^1.0.0",
    "@builder.io/partytown": "^0.6.4",
    "astro": "^1.0.5",
    "astro-compress": "^1.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "*.{js,json,ts,tsx,astro}": [
      "pnpm prettier -w src/**/*.astro",
      "npx stylelint public/**/*.css",
      "eslint src/*.{js,jsx,ts,tsx,astro} --fix-dry-run --no-error-on-unmatched-pattern"
    ]
  }
}
