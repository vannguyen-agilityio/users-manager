---
import SocialIcon from 'components:SocialIcon/index';

export interface TikTokIconProps {
  className?: string;
  href?: string;
}

const { className = '', href = '' } = Astro.props;
---

<SocialIcon
  className={className}
  href={href}
  imageUrl="assets/images/icons/tiktok.svg"
  imageAlt="DogeCard TikTok"
/>
