---
import SocialIcon from 'components:SocialIcon/index';

export interface TwitterIconProps {
  className?: string;
  href?: string;
}

const { className = '', href = '' } = Astro.props;
---

<SocialIcon
  className={className}
  href={href}
  imageUrl="assets/images/icons/twitter.svg"
  imageAlt="DogeCard Twitter"
/>
