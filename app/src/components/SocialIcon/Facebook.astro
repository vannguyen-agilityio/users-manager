---
import SocialIcon from 'components:SocialIcon/index';

export interface FacebookIconProps {
  className?: string;
  href?: string;
}

const { className = '', href = '' } = Astro.props;
---

<SocialIcon
  className={className}
  href={href}
  imageUrl="assets/images/icons/facebook.svg"
  imageAlt="DogeCard Facebook"
/>
